# ../sim/top_wv/waveform_top.wlf opened as dataset "waveform_top"
add wave -position insertpoint  \
waveform_top:/apb_top_tb/PCLK \
waveform_top:/apb_top_tb/PRESETn \
waveform_top:/apb_top_tb/PSEL \
waveform_top:/apb_top_tb/PENABLE \
waveform_top:/apb_top_tb/PWRITE \
waveform_top:/apb_top_tb/PADDR \
waveform_top:/apb_top_tb/PWDATA \
waveform_top:/apb_top_tb/PRDATA \
waveform_top:/apb_top_tb/IRQ \
waveform_top:/apb_top_tb/PREADY \
waveform_top:/apb_top_tb/aux_in \
waveform_top:/apb_top_tb/io_pad \
waveform_top:/apb_top_tb/ext_clk_pad_i \
waveform_top:/apb_top_tb/flag \
waveform_top:/apb_top_tb/temp \
waveform_top:/apb_top_tb/drive_enable
add wave -position insertpoint  \
waveform_top:/apb_top_tb/i1/reg_instance/rgpio_ctrl
find drivers -source -time {1724 ns} -cause waveform_top:/apb_top_tb/i1/reg_instance/rgpio_ctrl
